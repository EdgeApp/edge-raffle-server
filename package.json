  "license": "MIT",
  "scripts": {
    "configure": "configure",
    "build.web": "webpack --mode production --no-watch",
    "build.server": "tsc -p tsconfig.server.json",
    "prepare": "yarn clean && yarn build.server && yarn build.web",
    "start": "node lib/server/index.js",
    "start.dev": "node -r sucrase/register src/server/index.ts",
    "start.web": "webpack serve --mode development",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "rimraf lib dist",
    "precommit": "yarn lint && yarn tsc --noEmit",
    "postinstall": "yarn prepare"
  },
  "dependencies": {
    "@prosopo/react-procaptcha-wrapper": "^1.0.0",
