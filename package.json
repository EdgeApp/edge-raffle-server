    "cleaners": "^0.3.17",
    "cors": "^2.8.5",
    "edge-server-tools": "^0.2.23",
    "express": "^5.2.1",
    "nano": "^10.1.0",
    "nodemailer": "^8.0.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/cors": "^2.8.17",
